{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es6", "dom"],
      "module": "commonjs",
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "alwaysStrict": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "strictPropertyInitialization": true
    }
  }